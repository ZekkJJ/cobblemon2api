{
  "name": "cobblemon-los-pitufos-backend",
  "version": "1.0.0",
  "description": "API Backend para Cobblemon Los Pitufos - Servidor de Minecraft con Cobblemon",
  "main": "server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "dev:old": "nodemon server.js",
    "build": "tsc",
    "start": "node dist/server.js",
    "start:fallback": "node server.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "cobblemon",
    "minecraft",
    "pokemon",
    "api",
    "express"
  ],
  "author": "Los Pitufos",
  "license": "MIT",
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "groq-sdk": "^0.37.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mathjs": "^15.1.0",
    "mongodb": "^6.21.0",
    "socket.io": "^4.8.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.17.9",
    "@types/socket.io": "^3.0.1",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "fast-check": "^3.15.0",
    "mongodb-memory-server": "^11.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.7.2",
    "vitest": "^1.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
